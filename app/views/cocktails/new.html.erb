<div class="container">
<h1>Add a new cocktail</h1>

<%= simple_form_for @cocktail do |t| %>
<%= t.input :name, input_html: {id: :cocktail_search} %>
<div id="results" class="mt-3 mb-3 d-flex justify-content-between">
</div>
<%= t.hidden_field :image_url %>
<%= t.submit class: "btn btn-primary" %>
<% end %>


</div>
